    <ProjectReference Include="..\OpenTelekomCloud.Serverless.Function.Common.legacy\OpenTelekomCloud.Serverless.Function.Common.legacy.csproj" />
  </ItemGroup>
  

  <PropertyGroup>

    <Description>OpenTelekomCloud FunctionGraph .NET Core support - Common Test Utils.</Description>
    <AssemblyTitle>OpenTelekomCloud.Serverless.Function.Common.TestUtils</AssemblyTitle>

    <AssemblyName>OpenTelekomCloud.Serverless.Function.Common.TestUtils</AssemblyName>

    <Version>1.0.0.0</Version>

    <RootNamespace>OpenTelekomCloud.Serverless.Function.Common</RootNamespace>
    <PackageId>OpenTelekomCloud.Serverless.Function.Common.TestUtils</PackageId>
    <PackageTags>OpenTelekomCloud;OTC;FunctionGraph</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>

